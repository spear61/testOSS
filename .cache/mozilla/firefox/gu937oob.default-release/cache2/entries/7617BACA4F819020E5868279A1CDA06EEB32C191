function arcList () {
	$( '.arc_li' ).click( function () {
		var $_this = $( this );
		$_this.toggleClass( 'active' );
		$_this.siblings( '.arc_div' ).stop().slideToggle();
	} );
}
var tableColWidth = function () {
	var tableName = $( 'table' );
	var num = 'ë²ˆí˜¸';
	var width = 40;
	tableName.each( function () {
		var tableName = $( this );
		var tableNameCheck = tableName.find( 'th input[type=checkbox]' );
		var tableNameNum = tableName.find( 'th' ).text().indexOf( num );
		if(tableName.find( 'th' ).text() == "í•™ìƒëª…ì„±ë³„ì „í™”ë²ˆí˜¸íœ´ëŒ€í°E-ë©”ì¼ì£¼ì†Œ") return false;
		if ( tableNameCheck.length != 0 || tableNameNum != -1 ) {
			tableNameCheck.parents( 'th' ).css( 'width' ,width );
			$( 'table th' ).each( function () {
				var n = $( this ).text();
				var colValue = 'auto';
				var $_thisIndex = $( this ).index();
				var colGroupRemove = $( this ).parents( 'table' ).find( 'colgroup col:eq(' + $_thisIndex + ')' );
				if ( $( this ).html().indexOf( 'input' ) != -1 ) {
					$( this ).css( 'width' ,width - 10 );
					colGroupRemove.css( 'width' ,colValue );
				}
				switch ( n ) {
					case 'ë²ˆí˜¸' :
						$( this ).css( 'width' ,width );
						colGroupRemove.css( 'width' ,colValue )
						break;
					case 'í•™ë…„' :
						$( this ).css( 'width' ,width + 10 );
						colGroupRemove.css( 'width' ,colValue )
						break;
					case 'ë©”ëª¨' :
					case 'ê¸°ì—…í‰ê°€' :
					case 'êµìˆ˜í‰ê°€' :
					case 'ì´ì ' :
					case 'ì·¨ì—…ì—¬ë¶€' :
					case 'ì„­ì™¸ì£¼ì²´' :
					case 'ì°¸ì—¬íšŸìˆ˜' :
					case 'í˜‘ì•½ì„œ' :
						$( this ).css( 'width' ,width + 30 );
						colGroupRemove.css( 'width' ,colValue )
						break;
					case 'BlackList' :
					case 'IPPì „ë‹´êµìˆ˜' :
					case 'ë³´í—˜ê°€ì…' :
					case 'í•™ì êµ¬ë¶„' :
					case 'ì€í–‰ëª…' :
					case 'í†µì¥ì‚¬ë³¸' :
					case 'ë©´ì ‘ì¼ì •' :
					case 'ì„ ë°œì—¬ë¶€' :
					case 'ì¶œê·¼ë¶€' :
					case 'ë³´ê³ ì„œ' :
					case 'êµìˆ˜í‰ê°€í‘œ' :
					case 'ëŒ€í‘œì „í™”' :
					case 'ê¸°ì—…í‰ê°€í‘œ' :
					case 'ë³´ê³ ì„œì¶œë ¥' :
					case 'ê²°ê³¼ë³´ê³ ì„œ' :
						$( this ).css( 'width' ,width * 2 );
						colGroupRemove.css( 'width' ,colValue )
						break;
					case 'ì„œë¥˜ìƒíƒœ' :
					case 'ì„œì•½ë™ì˜ ì—¬ë¶€' :
						$( this ).css( 'width' ,width * 3 );
						colGroupRemove.css( 'width' ,colValue )
						break;
					case 'ê¸°ìˆ ëª…(ì¶œì›ëª…)' :
						$( this ).css( 'width' ,width * 8 );
						colGroupRemove.css( 'width' ,colValue )
						break;
					case 'í˜‘ì•½ê¸°ì—…ëª…' :
						$( this ).css( 'width' ,width * 7 );
						colGroupRemove.css( 'width' ,colValue )
						break;
					case 'í•™ê³¼' :
					case 'ì „ê³µ' :
						$( this ).css( 'width' ,width * 5 );
						colGroupRemove.css( 'width' ,colValue )
						break;
					case 'ì°¸ì—¬ë‚´ìš©' :
						$( this ).css( 'width' ,width * 15 );
						colGroupRemove.css( 'width' ,colValue )
						break;
				}
			} );
		}
	} );
}
