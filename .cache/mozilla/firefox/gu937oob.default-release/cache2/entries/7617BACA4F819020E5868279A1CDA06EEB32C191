	function snapScrollbar () {
		var posX = $( "#content_3 .mCSB_container" ).position().left ,closestX = findClosest( Math.abs( posX ) ,snapTo );
		$( "#content_3" ).mCustomScrollbar( "scrollTo" ,closestX ,{
			scrollInertia : 350 ,
			callbacks : false
		} );
	}
	function findClosest ( num ,arr ) {
		var curr = arr[ 0 ];
		var diff = Math.abs( num - curr );
		for ( var val = 0 ; val < arr.length ; val++ ) {
			var newdiff = Math.abs( num - arr[ val ] );
			if ( newdiff < diff ) {
				diff = newdiff;
				curr = arr[ val ];
			}
		}
		return curr;
	}
