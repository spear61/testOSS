	var len = 0;
	var accordionFun = {
		"accleave" : function () {
			$( '.accordion li' ).each( function ( index ) {
				$( this ).stop().animate( {
					width : '256px'
				} ,600 ,'easeInOutQuint' );
				$( this ).find( '.black' ).stop().animate( {
					opacity : '0' ,
					filter : 'alpha(opacity=0)'
				} ,600 ,function () {
					$( this ).hide();
				} );
				$( this ).find( '.accordion_bg' ).stop().animate( {
					left : bg_origin[ index ]
				} ,600 ,'easeInOutQuint' );
				$( this ).find( '.accordion_img' ).stop().animate( {
					right : img_origin[ index ]
				} ,600 ,'easeInOutQuint' );
				$( this ).find( '.accordion_form' ).stop().animate( {
					left : '-300px' ,
					opacity : '0'
				} ,800 ,'easeInOutQuint' );
			} );
		}
	};
