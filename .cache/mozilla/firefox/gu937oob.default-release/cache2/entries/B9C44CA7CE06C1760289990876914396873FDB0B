	imageSlideSetupSeq=54,cnvrsVe=1,stopTime=5,pcCo=2,cnvrsMth=SLIDE,pcMgWidth=100,isImageNoHandlr=true,isCnvrsHandlr=true,isPlayStopHandlr=true
