		//<!-- [해외구분 20200602]-->
		"compCountryCode" : null ,
		"type" : null,
		"func":null,
		//<!-- //[해외구분 20200602]-->
		"layout":function(form){
			var url = "/company.do";
			if(common.company.compType != null){
				url = "/company2.do";
			}else{
				url = "/company.do";
			}
			$.fun.ajax({
				"url":url,
				data:form?$(form).serialize():null,
				"success":function(data){
					$.fun.layout({
						"id":"company_layout",
						"title":"기관(법인)명 검색",
						"width":570,
						"content":$(data)
					});
				}
			});
			return false;
		},
		"bind":function(param){
			console.log(param);

			common.company.compSeq.val(param.compSeq);
			common.company.compName.val(param.compName);
			common.company.internCodeStr.val(param.internCodeStr);
//			common.company.compNum.val(param.compNum);
			if (common.company.compZipcode){
				common.company.compZipcode.val(param.compZipcode);
				common.company.compAddr1.val(param.compAddr1);
				common.company.compAddr2.val(param.compAddr2);
			}

			//<!-- [교수추가] -->
			if(common.company.jidProfId != null) {
				common.company.jidProfId.val(param.profId);
				common.company.jidProfDept.val(param.profDept);
				common.company.jidProfName.val(param.profName);
			}
			if(common.company.jimmProfId != null) {
				common.company.jimmProfId.val(param.profId);
				common.company.jimmProfName.val(param.profName);
				common.company.jimmProfDept.val(param.profDept);
			}

			//<!--[202103 COOP] 추가-->
			$("input[name='jcwWorkingDayCd[]']").prop("checked",false);
			if(param.workCompList != '') {

				for(i=0; i<param.workCompList.split(",").length; i++) {
					console.log(param.workCompList.split(",")[i]);
					id = "#jcwWorkingDayCd" +  param.workCompList.split(",")[i];
					console.log(id);
					$(id).prop("checked",true);
				}
			}
			//<!--//[202103 COOP] 추가-->

			//<!-- [국내/국외구분 202104] -->
			if ( param.compCountryCode != null ) {
				console.log("d");
				common.company.compCountryCode.val( param.compCountryCode );
				console.log($("#compCountryCode").val() + "==" + common.company.type)
				common.country.chn($("#compCountryCode").val(),common.company.type);
			}
			//<!-- //[국내/국외구분 202104] -->

			if(common.company.func) common.company.func(param);
			$("#company_layout").dialog('destroy').remove();
			return false;
		},
		"search":function(page){
			var form=document.compSearchForm;
			if(page) form.page.value=page;
			return common.company.layout(form);
		},
		"init":function(param){
			console.log(param)
			common.company.compSeq=param.compSeq;
			common.company.compName=param.compName;
			common.company.compType=param.compType;
			common.company.internCodeStr=param.internCodeStr;
			common.company.compZipcode=param.jidZipcode;
			common.company.compAddr1=param.jidAddr1;
			common.company.compAddr2=param.jidAddr2;


			//<!-- [교수추가] -->
			if(param.jidProfId != null) {
				common.company.jidProfId = param.jidProfId;
				common.company.jidProfName = param.jidProfName;
				common.company.jidProfDept = param.jidProfDept;
			}
			if(param.jimmProfId != null) {
				common.company.jimmProfId = param.jimmProfId;
				common.company.jimmProfName = param.jimmProfName;
				common.company.jimmProfDept = param.jimmProfDept;
			}
			//<!-- //[교수추가] -->

			//<!-- [국내/국외구분 202104] -->
			if ( param.compCountryCode != null ) {
				common.company.compCountryCode = param.compCountryCode;
			}
			if ( param.type != null ) {
				common.company.type = param.type;
			}
			//<!-- //[국내/국외구분 202104] -->

			if(param.func != null) common.company.func=param.func;

//			common.company.compNum=param.compNum;
			common.company.layout(null);
			return false;
		},
		"keydown":function(inp){
			$(inp).keydown(function(event){
				if(event.which==13)
					common.company.layout(document.compSearchForm);
			});
		}
