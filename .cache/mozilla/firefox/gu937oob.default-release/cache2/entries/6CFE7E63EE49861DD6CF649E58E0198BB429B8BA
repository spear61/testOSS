	"dateChange" : function(jidSeq,target){
		console.log("ddd")
		console.log(jidSeq);
		console.log(target);
		if($("input[name='"+target+"']").val() == "") {
			$.fun.alert({
				"content":"변경할 날짜를 입력해주세요"
			});
			return false;
		}
		var jidEDate = $("input[name='"+target+"']").val();
		$.fun.confirm({
			"content":"실습기간 마지막일자를 변경하시겠습니까?",
			"action":function(){
				$.fun.ajax({
					"url":"/dateChange.do",
					"data": {"jidSeq" : jidSeq
							,"jidEDate" : jidEDate
							},
					"success":function(data){
						data=$.fun.json_encode(data);
						console.log(data);

						if(data["result"] > 0){
							$.fun.alert({
								"content":"요청하신 작업이 완료되었습니다.",
							});
							location.reload();
						}else{
							$.fun.alert({
								"content":"요청이 실패되었습니다."
							});
						}
					}
				});
				return false;
			}
		});
	},
