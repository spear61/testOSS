	},
	"totalVisitReport" : function (jsSeq) {
		var checkboxInfo = $.fun.checkboxInfo( "jirSeq[]" );
		var jirNum = [];
		if(checkboxInfo.checked == 0) {
			$.fun.alert( {
				"content" : "학생을 선택해주세요."
			} );
			return false;
		}
		$("input[name='jirSeq[]']:checked").each(function(idx,item){
			jirNum.push($(item).next().val());
		});
		var jirSeq = checkboxInfo.checkedList;
		window.open( print_.url.totalVisitReport + "?jirSeq=" + jirSeq + "&jirNum=" + jirNum + "&jsSeq=" + jsSeq, "attendance" ,'toolbar=no, location=no, left=0, top=0, directories=no, status=no, menubar=no, scrollbars=yes, resizable=no, copyhistory=no, width=700px, height=850px' );
	}

