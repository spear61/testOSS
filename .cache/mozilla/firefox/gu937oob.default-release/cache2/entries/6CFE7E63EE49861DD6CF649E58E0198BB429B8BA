	"attsetNew" : function ( val, group ) {
		console.log(val)
		console.log(group)
		nowDate = new Date();
		year = nowDate.getFullYear();
		month = nowDate.getMonth()+1;
		day = nowDate.getDate()
		if(month < 10) month = '0'+month;
		if(day < 10) day = '0'+day;
		now = year + "-" + month + "-" + day;

		if (group == "4") {
			$( ".tbl_calendar" ).find( "tbody td" ).each( function ( i ) {
				if ( $( this ).find( "input[name='workday[]']" ).length > 0 ) {
					if ( $( this ).find( "input[name^='workday']:checked" ).length < 1 ) {

						$( this ).find( "input[name^='workday'][value='" + val + "']" ).prop( "checked" ,"true" );
						$( this ).find( "input[name^='workday'][value='" + val + "']" ).click();
					}
				}
			} );
		} else {
			$( ".tbl_calendar" ).find( "tbody td" ).each( function ( i ) {
				if ( $( this ).find( "input[name='workday[]']" ).length > 0 ) {
					if ( $( this ).find( "input[name^='workday']:checked" ).length < 1 ) {
						if(now >= $( this ).find( "input[name^='workday']").val()) {
							$( this ).find( "input[name^='workday'][value='" + val + "']" ).prop( "checked" ,"true" );
							$( this ).find( "input[name^='workday'][value='" + val + "']" ).click();
						}
					}
				}
			} );
		}

		$( "input[name^='workday']" ).each( function () {
			if ( $( this ).is( ":checked" ) == true && $( this ).val() == "Y" ) {
				self.readonlyChn( this ,false );
				$( this ).parent().find( "[name='jiwHour[]']" ).val( 8 );
				$( this ).parent().find( "[name='jiwHourExtend[]']" ).val( 0 );
			}
		} );
	} ,
