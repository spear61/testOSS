	"att" : {
		"init" : function(target) {
			$(target).each(function(){
				startDate = $(this).find("input[name='jidcSDate']").val();
				endDate = $(this).find("input[name='jidcEDate']").val();
				title = $(this).find("input[name='jidcTitle']").val();
				common.att.closeData(startDate,endDate,title);
			});
		},
		"closeData" : function(startDate,endDate,title){
			$(".tbl_calendar").find("input[name='workday[]']").each(function(){
				if(startDate <= $(this).val() && endDate >= $(this).val()) {
					$( "input:radio[name='workday"+$(this).val()+"']:radio[value='S']" ).prop( "checked" ,true );
					$( "input:radio[name='workday"+$(this).val()+"']" ).attr( "onclick" ,"return false;" );
					$( "input:radio[name='workday"+$(this).val()+"']" ).parent().prepend( "<span class='dk_alert_r'>"+title+"</span>" );
					$( "input:radio[name='workday"+$(this).val()+"']" ).parent("td").css("background-color" , "#f5f5f5");
				}
			});
		}
	},
	//<!-- [참여신청서 휴일관리] -->
	"comma" : function ( num ) {
		num = num.replaceAll( "," ,"" );
		console.log( num );
		var len ,point ,str;
		num = num + "";
		point = num.length % 3;
		len = num.length;
		str = num.substring( 0 ,point );
		while ( point < len ) {
			if ( str != "" ) str += ",";
			str += num.substring( point ,point + 3 );
			point += 3;
		}
		return str;
	} ,
	"commaVal" : function ( target ,num ) {
		$( target ).val( common.comma( num ) );
	},
	//<!-- 이력서가져오기 [20200506] -->
	"import" : {
		"list" : function(url,title,stNum,mode) {
			var buttons = { "닫기" : function () { $( this ).dialog( 'destroy' ).remove(); }}
			$.fun.ajax( {
				"url" : url ,
				"data" : {
					"stNum" : stNum
					,"mode" : mode
				} ,
				"success" : function ( data ) {
					if($(data).find("input[name='recSeq[]']").length > 0) {
						buttons = {
									"저장" : function () { common.import.proc(stNum,mode); } ,
									"닫기" : function () { $( this ).dialog( 'destroy' ).remove(); }
								}
					}

					$.fun.layout( {
						"id" : "RECORD_LAYOUT" ,
						"title" : title + " 가져오기" ,
						"width" : 900 ,
						"content" : $( data ) ,
						"buttons" : buttons
					} );
				}
			} );
			return false;
		},
		"proc" : function(stNum,mode){
			var checkboxInfo=$.fun.checkboxInfo("recSeq[]");
			if(!checkboxInfo.checked){
				$.fun.alert({
					"content":"대상을 선택 해주세요.",
					"action":function(){
						checkboxInfo.firstNode.focus();
					}
				});
				return false;
			}
			var data = {};
			data.recordJSON = $.fun.json_decode( checkboxInfo.checkedList );
			data.stNum = stNum;
			data.mode = mode;
			console.log(data);
			$.fun.confirm( {
				"content" : "선택한 항목을 추가하시겠습니까?" ,
				"action" : function () {
					$.fun.ajax( {
						"url" : "/common/record/proc.do",
						"data" : data ,
						"success" : function ( data ) {
							data = $.fun.json_encode( data );
							console.log( data );
							if ( data[ "result" ] ) {
								$.fun.alert( {
									"content" : "요청하신 작업이 완료되었습니다." ,
									"action" : function () {
										location.reload();
									}
								} );
							} else {
								$.fun.alert( {
									"content" : "요청이 실패되었습니다."
								} );
							}
						}
					} );
					return true;
				}
			} );
			return false;
		},
		// 언어능력
		"init" : function( mode,stNum,title ) {
			common.import.list("/common/layout/stdRecord.do",title,stNum,mode);
		}
	},
	//<!-- //이력서가져오기 [20200506] -->
	"numChk" : function (req) {
		if ( ( event.keyCode > 47 && event.keyCode < 58 ) || ( event.keyCode == 8 ) || ( event.keyCode == 9 ) || ( event.keyCode > 95 && event.keyCode < 106 ) || ( event.keyCode == 46 ) ) {} else {
			$( ".onlynum" ).keyup( function () {
				$( this ).val( $( this ).val().replace( /[^0-9]/g ,"" ) );
			} );
			$.fun.alert( {
				"content" : "숫자만 입력해주세요."
			} );
		}
	} ,
	"multiFile" : {
		"upload":function(obj, indexCnt,level){
			common.upload(function(data){
				var root=$(obj).parents(".contentArea");
				if(!root.find("[name='comUpload"+level+"_"+indexCnt+"']").length){
					root.append("<input type='hidden' name='comUpload"+level+"_"+indexCnt+"'/>");
				}
				console.log(level);
				console.log(".comFileNm"+level+"_"+indexCnt);
				root.find("[name='comUpload"+level+"_"+indexCnt+"']").val($.fun.json_decode(data));
				root.find(".comFileNm"+level+"_"+indexCnt).val(data.orignlFileNm);
			});
			return false;
		},
		"addFile":function(obj, cnt, level,filNmCls){
			var root=$(obj).parents(".multiMain");
			var fileArea = root.find(".contentArea").first().clone();
			var len = root.find( ".contentArea").length;
			console.log(len);
			fileArea.addClass("mt_4");
			fileArea.find( "input[name='dataSeq"+level+"[]']").val("0");
			fileArea.find( "input[name='content"+level+"[]']").val("");
			fileArea.find( "input[name='comFileSn"+level+"_0']").attr("name","comFileSn"+level+"_"+len).val("");
			fileArea.find( "input[name='comFileSn"+level+"_0']").attr("name","comFileSn"+level+"_"+len);

			fileArea.find( ".comFileNm"+level+"_0").attr("class",filNmCls+" mr_4 comFileNm"+level+"_"+len).val("");
			fileArea.find( "input[name='comFileSn"+level+"_0']").attr("name","comFileSn"+level+"_"+len);
			fileArea.find( ".find").attr("onclick","common.multiFile.upload(this,'"+len+"','"+level+"')");
			fileArea.find( ".add_item").attr("onclick","common.multiFile.addFile(this,'"+len+"','"+level+"','"+filNmCls+"')");
			fileArea.find( "input[name^='comUpload"+level+"']").remove();
			fileArea.append("<a href=\"#;\" onclick=\"return common.multiFile.delFile(this,'"+len+"','"+level+"');\" class=\"delete_entry mr_4\"><span class=\"blind\">항목삭제</span></a>")
			root.append( fileArea );
			return false;
		},
		"delFile" : function ( obj, cnt, level ) {
			var root = $( obj ).parents(".contentArea");
			var fcnt = $( obj ).parents(".contentArea").index();

			// 삭제 시퀀스
			var delSeq = root.find("input[name^='dataSeq']").clone();
			delSeq.prop("name",delSeq.attr("name").replace("data","del"));
			root.parents(".multiMain").find(".delArea"+level).append(delSeq);

			// 삭제파일
			if(root.find("input[name^='comFileSn']").length > 0) {
				var delFile = root.find("input[name^='comFileSn']").clone();
				delFile.prop("name",delFile.attr("name").replace("com","del"))
				$(obj).parents(".multiMain").find(".delArea"+level).append(delFile);
			}
			root.remove();

			$(obj).parents(".multiMain").find(".contentArea").each(function(i,e){
				$(this).find("input[name^=comFileSn]").attr("name","comFileSn"+cnt+"_"+i);											// 파일시퀀스 변경
				$(this).find(".comFileNm").attr("class","comFileNm"+cnt+"_"+i);														// 파일이름명 변경
				$(this).find(".find").attr("onclick","return common.multiFile.upload(this,'"+i+"','"+level+"')");				// 파일업로드(이벤트)
				$(this).find(".add_item").attr("onclick","return common.multiFile.addFile(this,'"+i+"','"+level+"')");			// 파일추가(이벤트)
				$(this).find("input[name^=comUpload]").attr("name","comUpload"+cnt+"_"+i);											// 파일업로드명 변경
				if(i > 0) {
					$(this).find(".delete_entry").attr("onclick","return common.multiFile.delFile(this,'"+i+"','"+level+"')");	// 삭제(이벤트)
				}
			});
			return false;

		} ,
	}

};

function exDeptCallback(){

}
