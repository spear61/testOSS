					}
				});
			});
			return false;
			} ,
		"proc":function(){
			var form=document.degreeForm;
			var data=$(form).serializeArray();
			$.fun.confirm({
				"content":"개인정보 수집·이용동의에 동의하시겠습니까?",
				"action":function(){
					$.fun.ajax({
						"url":"/common/agreeProc.do",
						"data":data,
						"success":function(data){
							data=$.fun.json_encode(data);
							console.log(data);
							if(data["result"]>-1){
								$.fun.alert({
									"content":"동의하였습니다.",
									"action":function(){
										location.reload();
									}
								});
							}else{
								$.fun.alert({
									"content":"요청이 실패되었습니다."
								});
							}
						}
					});
					return true;
				}
			});
			return false;
		},
