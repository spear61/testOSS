	// <!-- [enterTab]-->
	 "enterTab" : {
		"init" : function(target) {
			$(target).find("input[type='text'],input[type='password']").each(function(i){
				enterList[i] = $(this).attr("name");
			});
			enterCnt = $(target).find("input[type='text'],input[type='password']").length;
			common.enterTab.action(target);
		},
		"action" : function(target) {
			$(document).on("keyup","input", function(event){
				if(event.keyCode==13){
					var $thisName = $(this).attr("name");
					$.each(enterList,function(index,entry){
						if(entry == $thisName && index < enterCnt-1) {
							console.log(index);
							console.log(entry);
							$(target).find("input[name='"+enterList[index+1]+"']").focus();
						}
					});
				}
			})
		}
	},
