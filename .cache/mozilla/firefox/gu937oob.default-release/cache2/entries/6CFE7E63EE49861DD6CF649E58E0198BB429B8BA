};
var commNcs = {
		"init":function(){
			$.fun.rowspan("#ncsTable", 0);
			$.fun.rowspan("#ncsTable", 1);
			commNcs.btnCheck();
			if($("#ncsDept") != null){
				commNcs.ncsDept = $("#ncsDept").val();
			}
		},
		"jobNcs":function(ncsCode, jidSeq, jimSeq, deptCode,jsYear){
			$.fun.ajax({
				"url":"/jobNcs.do",
				"data":{
					"ncsCode":ncsCode,
					"jidSeq":jidSeq,
					"jimSeq":jimSeq,
					"deptCode":deptCode,
					"jsYear":jsYear
				},
				"success":function(data){
					$.fun.layout({
						"id":"ncs_LAYOUT",
						"title":"직업기초능력",
						"width":1200,
						"content":data,
						"buttons":{
							"닫기":function(){
								$(this).dialog('destroy').remove();
							}
						}
					});
				}
			});
			return false;
		},
		//선택된 운영계획서 주차
		"ncsW":null,
		//선택된 운영계획서 학과
		"ncsDept":null,
		//운영계획서 주차 선택시 저장
		"ncsWfn":function(ncsW){
			commNcs.ncsW = ncsW;
		},
		//선발대상선택 유무 체크
		"ncsDeptMsg":function(){
			if(commNcs.ncsDept != null){
				if(commNcs.ncsWmsg()) return true
				else return false;
			}else{
				$.fun.alert({
					"content":"선발대상을 선택해주세요."
				});
				return true;
			}
		},
		//운영계획서 주차선택 유무 체크
		"ncsWmsg":function(){
			if(commNcs.ncsW != null){
				return false;
			}else{
				$.fun.alert({
					"content":"주차를 선택해주세요."
				});
				return true;
			}
		},
		//수행준거 추가
		"ncsAdd":function(obj){
			if(commNcs.ncsDeptMsg()) return;
			var text = $(obj).parent().prev().text();
			var seq = $(obj).attr("id").replace("addBtn", "");
			var str = "";
			str = str + '<tr><td><input type="hidden" name="ncsCode_' + $(commNcs.ncsW).val() + '[]" value="' + seq + '"/>';
			str = str + '<input type="hidden" name="ncsText_' + $(commNcs.ncsW).val() + '[]" value="' + text + '"/>';
			//str = str + '<tr><td><input type="hidden" name="ncsData" value="' + seq + '"/>';
			str = str + text + '</td><td class="tac last"><a href="javascript:;" class="btn_inner ncsDel" onclick="commNcs.ncsDel(this, \'' + seq + '\')"><span class="btn_txt">삭제</span></a></td></tr>';
			$(commNcs.ncsW).parent().next().find("tbody").append(str);
			commNcs.btnCheck();
		},
		//수행준거 다중 추가
		"ncsAddArr":function(obj, num){
			if(commNcs.ncsDeptMsg()) return;
			seq = ($(obj).attr("id") + "").replace("addBtn", "")
			$(".addBtn").each(function(index){
				var aSeq = ($(this).attr("id") + "").replace("addBtn", "").substring(0,num);
				//중분류값이 같고 %% 키값이 2자리이상 && 버튼이 숨겨지지않은것
				if(seq == aSeq && ($(this).attr("id") + "").replace("addBtn", "").length > 2 && $(this).css("display") != "none"){
					commNcs.ncsAdd($(this), $(this).attr("id").replace("addBtn", ""));
				}
			});
			commNcs.btnCheck();
		},
		//수행준거삭제
		"ncsDel":function(obj, seq){
			$(obj).closest("tr").remove();
			$("#addBtn" + seq).show();
			$("#addBtn" + seq.substring(0, 2)).show();
			$("#addBtn" + seq.substring(0, 1)).show();
		},
		// 수행준거 리스트 추가
		"ncsPlusAdd" : function(req){
			var target = "#ncsTbody";
			var lens = $(".plusData").length;
			var lastName="";
			if($(".plusData").length == 0) {
				deptCode="01";
			} else {
				lastName = parseInt($(".plusData:eq("+(lens-1)+")").find("a").attr("id").substring(18,20)) + 1;
				if(lastName.toString().length <= 1) deptCode = '0' + lastName;
			}
			var str = str + "<tr>"
				+ '<td colspan="3">' + $(req).parent().prev().find("input[name='contents']").val()
				+ '<input type="hidden" name="contentsPlus[]" value="'+ $(req).parent().prev().find("input[name='contents']").val() +'">'
				+ '<input type="hidden" name="deptCodePlus[]" value="' + deptCode + '">'
				+ '</td>'
				+ '<td class="tac last plusData">'
				+ '<a href="javascript:;" id="addBtn' + '111111111111' + deptCode + '" class="btn_inner addBtn" onclick="commNcs.ncsAdd(this);"><span class="btn_txt">적용</span></a>'
				+ '</td>'
				+ '</tr>';
			$(target).append(str);
		},
		"ncsPlusDel" : function(req){
			$(req).parents("tr").remove();
		},
		"ncsProc":function(){
			document.searchForm.saveAction.value="ncs";
			reqComp.proc();
		},
		//선택된 운영계획서에있는 것들 체크해서 수행준거에서 버튼 삭제
		"btnCheck":function(){
//			$(".ncsDel").each(function(index){
//				var seq = $(this).parent().prev().find("input").eq(0).val();
//				$("#addBtn" + seq).hide();
//			});
//			commNcs.btnCheckArr(1);
//			commNcs.btnCheckArr(2);
		},
		"btnCheckArr":function(num){
			$(".ncsDel").each(function(index){
				var seq = $(this).parent().prev().find("input").eq(0).val().substring(0,num);
				var chk = true;
				$(".addBtn").each(function(index){
					var aSeq = ($(this).attr("id") + "").replace("addBtn", "");
					if(seq == aSeq.substring(0,num) && aSeq.length > 2 && $(this).css("display") != "none"){
						chk = false;
					}
				});
				if(chk){
					$("#addBtn" + seq.substring(0,num)).hide();
				}
			});
		}
	};
$.fn.serializeObject=function(){
	var o={};
	var a=this.serializeArray();
	$.each(a,function(){
		if(o[this.name]!==undefined){
			if(!o[this.name].push){
				o[this.name]=[
					o[this.name]
				];
			}
			o[this.name].push(this.value||'');
		}else{
			o[this.name]=this.value||'';
		}
	});
	return o;
};
$.fn.setObject=function(o){
	var frm=this;
	$.each(o,function(n,v){
		var inp=$(frm).find("[name='"+n+"']");
		if(o[n].push){
			$.each(inp,function(i,v){
				this.value=o[n][i];
			});
		}
		inp.val(v);
	});
};
if(navigator.userAgent.indexOf("WebKit")==-1) var console={
	log:function(){}
};
$(document).ready(function(){
	jqueryEvent.bind($("BODY"));
	String.prototype.replaceAll=function(_findValue,_replaceValue){
		return this.replace(new RegExp(_findValue,"gi"),_replaceValue);
	};

	// 공백을 '-' 으로 변경
	$(".tbl_type01 tbody td").each(function(){
		if(!$(this).html().trim()) $(this).html("-");
	});
});
