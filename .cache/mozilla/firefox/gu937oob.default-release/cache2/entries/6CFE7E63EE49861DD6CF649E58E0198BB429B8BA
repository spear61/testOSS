	"bankMoney" : function ( jimSeq,bankMoney ) {
		if(bankMoney == "") {
			$.fun.alert({"content" : "기업체 실습비 지급금액을 입력해주세요"});
			return false;
		}
		$.fun.confirm( {
			"content" : "저장하시겠습니까?" ,
			"action" : function () {
				$.fun.ajax( {
					"url" : "/bankProc.do" ,
					"data" : {
						"jimSeq" : jimSeq ,
						"bankMoney" : bankMoney.replace(/,/gi, "")
					} ,
					"success" : function ( data ) {
						data = $.fun.json_encode( data );
						if ( data[ "result" ] ) {
							$.fun.alert( {
								"content" : "요청하신 작업이 완료되었습니다." ,
								"action" : function () {
									location.reload();
								}
							} );
						} else {
							$.fun.alert( {
								"content" : "요청이 실패되었습니다."
							} );
						}
					}
				} );
				return true;
			}
		});
	} ,
	"comFileUpload" : {
		"init" : function(dataSeq,uploadName,fileSnName,form,url,type){
			var formTag = "";
			if($("body").find(form).length < 1) {
				formTag = '<form name="'+form+'" id="'+form+'" method="get" action="/">';
				formTag += '<input name="dataSeq" value="'+dataSeq+'" />';
				formTag += '<input type="hidden" name="'+uploadName+'" />';
				formTag += '<input type="hidden" name="'+fileSnName+'" value="" />';
				formTag += '<input type="hidden" name="type" value="'+type+'" />';
				formTag += '</form>';
				$("body").append(formTag);
			}
			common.comFileUpload.upload(uploadName, form, url);
		},
		"upload" : function( uploadName ,form, url){
			/*console.log(uploadName)
			console.log(jimSeq)
			console.log(form)
			console.log(url)*/
			common.upload( function ( data ) {
				$("#" + form ).find( "[name='" + uploadName + "']" ).val( $.fun.json_decode( data ) );
				/*$("#" + form ).find( "[name='jimSeq']" ).val( jimSeq );*/
				$.fun.ajax( {
					"url" : url ,
					"data" : $("#" + form ).serialize() ,
					"success" : function ( data ) {
						$.fun.alert({
							"content":"업로드가 완료 되었습니다.",
							"action" : function () {
								location.reload();
							}
						});
					}
				} );
			} );
			return false;
		},
		"del" : function ( dataSeq,delName,url,type ) {
			var data = "";
			if(delName == "newOpFileDel") data = {"dataSeq":dataSeq,"newOpFileDel":"Y","type":type};
			if(delName == "insuFileDel") data = {"dataSeq":dataSeq,"insuFileDel":"Y","type":type};
			if(delName == "stAgreementFileDel") data = {"dataSeq":dataSeq,"stAgreementFileDel":"Y","type":type};
			if(delName == "jimPresentationFileDel") data = {"dataSeq":dataSeq,"jimPresentationFileDel":"Y","type":type};
			console.log(data);
			$.fun.confirm( {
				"content" : "파일을 삭제하시겠습니까?" ,
				"action" : function () {
					$.fun.ajax( {
						"url" : url ,
						"data" : data ,
						"success" : function ( data ) {
							data = $.fun.json_encode( data );
							console.log( data );
							if ( data[ "result" ] > -1 ) {
								$.fun.alert( {
									"content" : "삭제되었습니다." ,
									"action" : function () {
										location.reload();
									}
								} );
							} else {
								$.fun.alert( {
									"content" : "요청이 실패되었습니다."
								} );
							}
						}
					} );
					return true;
				}
			} );
			return false;
		} ,
	},
	"compValuation" : function ( jimSeq ) {
		$.fun.ajax( {
			"url" : "/common/layout/compValuation.do" ,
			"data" : {
				"jimSeq" : jimSeq
			} ,
			"success" : function ( data ) {
				$.fun.layout( {
					"id" : "compValuation_LAYOUT" ,
					"title" : "기관 피드백" ,
					"width" : 700 ,
					"content" : jqueryEvent.bind( data ) ,
					"buttons" : {
						"닫기" : {
							"text" : '닫기' ,
							'class' : 'close' ,
							"click" : function () {
								$( this ).dialog( 'destroy' ).remove();
								if ( conf.closeAction ) {
									conf.closeAction();
								}
							}
						}
					}
				} );
			}
		} );
		return false;
	} ,
