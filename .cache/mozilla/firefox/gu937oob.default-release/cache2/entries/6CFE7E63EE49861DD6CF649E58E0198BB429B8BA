	"daumZipcode" : function ( zipCode ,addr1 ) {
		daum.postcode.load( function () {
			new daum.Postcode( {
				oncomplete : function ( data ) {
					console.log( data );
					if ( data.userSelectedType == 'R' ) {
						$( zipCode ).val( data.zonecode );
						$( addr1 ).val( data.address );
					} else {
						$( zipCode ).val( data.zonecode );
						$( addr1 ).val( data.jibunAddress );
					}
				}
			} ).open();
		} );
	} ,
	"upload":function(action, type){
		if(type == null) type = "";
		$.fun.ajax({
			"url":"/fileUploadLayout.do",
			"success":function(data){
				$.fun.layout({
					"id":"file_upload_layout",
					"title":"파일업로드",
					"width":300,
					"content":jqueryEvent.bind($(data)),
					"action":function(data){
						var form=document.file_upload_form;
						$(form).attr({
							"method":"post",
							"action":"/upload" + type + ".do"
						});
						$.fun.ajaxSubmit({
							form:form,
							paramType:"text",
							action:function(data){
								$("#file_upload_layout").dialog('destroy').remove();
								action($.fun.json_encode(data));
							}
						});
					}
				});
			}
		});
		return false;
	},
