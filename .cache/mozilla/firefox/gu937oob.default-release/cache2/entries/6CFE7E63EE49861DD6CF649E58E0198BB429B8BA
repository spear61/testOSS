//<!-- [국내/국외구분 202104] -->
var param={};
var paramCp={};
//<!-- //[국내/국외구분 202104] -->
var common={
	"creditNotice" : function () {
		$.fun.ajax( {
			"url" : "/common/layout/creditNotice.do",
			"data" : {} ,
			"success" : function ( data ) {
				$.fun.layout( {
					"id" : "creditNotice_layout" ,
					"title" : "성적이관안내" ,
					"width" : 550 ,
					"content" : jqueryEvent.bind( $( data ) ) ,
					"buttons" : {
						"닫기" : function () {
							$( this ).dialog( 'destroy' ).remove();
						}
					}
				} );
			}
		} );
		return false;
	} ,
	"creditHisList" : function (jimSeq) {
		$.fun.ajax( {
			"url" : "/common/layout/creditHisList.do",
			"data" : {"jimSeq":jimSeq} ,
			"success" : function ( data ) {
				$.fun.layout( {
					"id" : "creditNotice_layout" ,
					"title" : "성적변경이력" ,
					"width" : 700 ,
					"content" : jqueryEvent.bind( $( data ) ) ,
					"buttons" : {
						"닫기" : function () {
							$( this ).dialog( 'destroy' ).remove();
						}
					}
				} );
			}
		} );
		return false;
	} ,
	"sms" : function(param){
		$.fun.ajax( {
			"url" : "/smsProc.do" ,
			"data" : param ,
			"success" : function ( data ) {
				data = $.fun.json_encode( data );
				if ( data[ "result" ] ) {
					$.fun.alert( {
						"content" : "요청하신 작업이 완료되었습니다." ,
						"action" : function () {
							location.reload();
						}
					} );
				} else {
					$.fun.alert( {
						"content" : "요청이 실패되었습니다."
					} );
				}
			}
		} );
		return true;
	},
	//<!-- [국내/국외구분 202104] -->
	"country" : {
		"init" : function(type) {
			// [공통영역] 데이터 저장 및 타겟 태그 생성
			$(".countryFamily").each(function(i,e){
				$(this).wrap('<span class="countryFamilyArea"></span>');
				param[e.name] = this;
				paramCp[e.name] = $(this).clone();
			});

			if(type == "COMP") {
				// 추가개발(아이디체크)
				/*if($("#writeForm").find("input[name='compSeq']").val() == "") {
					$("#compId").each(function(i,e){
						$(this).wrap('<span class="countryFamilyArea"></span>');
						param[e.name] = this;
						paramCp[e.name] = $(this).clone();
					});
				}*/
				// 추가개발(아이디체크)

				common.country.data.countryAdd1 = $(".countryAdd1").html();				// 등록번호 태그저장
				common.country.data.countryAdd2 = $(".countryAdd2").html();				// 사업장소재지 태그저장
				common.country.chn($("select[name='compCountryCode']").val(),"COMP");	// 적용
			} else if(type == "DEPT") {
				common.country.data.countryAdd2 = $(".countryAdd2").html();				// 사업장소재지 태그저장
				common.country.chn($("input[name='compCountryCode']").val(),"DEPT");	// 적용
			} else if(type == "JOIN") {
				common.country.data.countryAdd2 = $(".countryAdd2").html();				// 사업장소재지 태그저장
				common.country.chn($("input[name='compCountryCode']").val(),"JOIN");	// 적용
			}

		},
		"data" : {
			"countryAdd1" : "",
			"countryAdd2" : ""
		},
		"chn" : function(countryVal,type) {
			console.log(countryVal + "//" +  type)
				if(countryVal == "B") {

					$.each(param,function(index,entry){

						if($(entry).attr("class").indexOf("'required':true,") > -1 ) {
							console.log("aaa");
							$(entry).attr("class",$(entry).attr("class").replace("'required':true,",""));		// 필수제거
						}
						if($(entry).attr("class").indexOf("{'required':true}") > -1 ) {
							console.log("bbb");
							$(entry).attr("class",$(entry).attr("class").replace("{'required':true}",""));		// 필수제거
						}
						if($(entry).attr("class").indexOf("{'ime':'tel'}") > -1 ) {
							console.log("ccc");
							$(entry).attr("class",$(entry).attr("class").replace("{'ime':'tel'}",""));			// 전화형식제거
						}

						if($(entry).attr("class").indexOf("'ime':'compNo'") > -1 ) {
							console.log("ccc");
							$(entry).attr("class",$(entry).attr("class").replace("'ime':'compNo'",""));			// 사업자등록번호형식제거
						}
						$(entry).removeAttr("maxLength");			// maxLength 제거
						common.country.chnTh(entry.name,countryVal)	// 화면필수제거
					});
				} else {
					$.each(param,function(index,entry){
						$(entry).attr("class",$(paramCp[entry.name]).attr("class"));				// 본 class 변경
						$(entry).attr("maxLength",$(paramCp[entry.name]).attr("maxLength"));		// 본 maxLength 변경
						common.country.chnTh(entry.name,countryVal);
						// 추가개발(아이디체크)
						/*if(type == "COMP" && $(entry).attr("id") == "compId") {
							$(entry).attr("readonly",true);
						}*/
						// 추가개발(아이디체크)
					});
				}

				if(type == "COMP") {
					common.country.chnHtml.comp(countryVal);	// 항목 추가
					common.country.chnHtml.addr(countryVal,"compZipcode","compAddr1","compAddr2");	// 주소 변경
				} else if(type == "DEPT") {
					common.country.chnHtml.addr(countryVal,"compDeptZipcode","compDeptAddr1","compDeptAddr2");	// 주소 변경
				} else if(type == "JOIN") {
					common.country.chnHtml.addr(countryVal,"jidZipcode","jidAddr1","jidAddr2");	// 주소 변경
				}
				common.country.bind(countryVal);			// 바인드
		},
		// 화면필수제거
		"chnTh" : function(name,countryVal){
			if (countryVal == "B") {
				$("input[name='"+name+"']").parent().parent().prev().find("span").removeClass("required");
			}  else {
				if($("input[name='"+name+"']").parent().parent().prev().find("span").hasClass("required") == false) {
					$("input[name='"+name+"']").parent().parent().prev().find("span").addClass("required");
				}
			}
		},
		// 항목변경
		"chnHtml" : {
			"addr" : function(countryVal,zipcode,addr1,addr2){
				if(countryVal == "B") {
					console.log(zipcode)
					console.log(addr1)
					console.log(addr2)
					$(".countryAdd2").find("input[name='"+zipcode+"']").attr("readonly",false);
					$(".countryAdd2").find("input[name='"+zipcode+"']").attr("onclick","");
					$(".countryAdd2").find("input[name='"+zipcode+"']").attr("placeholder","우편번호(postal code) 입력해주세요.");
					$(".countryAdd2").find("input[name='"+zipcode+"']").removeClass("w_150")
					$(".countryAdd2").find("input[name='"+zipcode+"']").addClass("w_300")
					$(".countryAdd2").find(".btn_inner").remove();
					$(".countryAdd2").find("input[name='"+addr1+"']").attr("readonly",false);
					$(".countryAdd2").find("input[name='"+addr1+"']").attr("placeholder","주소(Address 1) 입력해주세요.");
					$(".countryAdd2").find("input[name='"+addr2+"']").attr("placeholder","주소(Address 2) 입력해주세요.");
				} else {
					$(".countryAdd2").html(common.country.data.countryAdd2);

				}
			},
			"comp" : function(countryVal) {
				// 실습기관의 추가되는 항목
				if(countryVal == "B") $(".countryAdd1").html(common.country.data.countryAdd1);
				else $(".countryAdd1").html("");
			},
			"dept" : function(countryVal) {
				// 부서의 추가되는 항목
			},
			"join" : function(countryVal) {
				// 참여신청서 추가되는 항목
			}
		},
		"bind" : function(countryVal) {
			$.each(param,function(index,entry){
				$(entry).parents(".countryFamilyArea").html(entry);		// bind 처리
			});
			jqueryEvent.bind($("#writeForm").find(".countryArea"));
		}
	},
