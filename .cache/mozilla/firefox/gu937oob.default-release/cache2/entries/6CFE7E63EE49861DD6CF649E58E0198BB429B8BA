//<!-- [202202 SYWORKS전용] -->
var syworks = {
	// STEP1 <항목 저장>
	"save" : function(saveTarget,saveStep) {
		var writeForm=document.writeForm;
		$(writeForm).find("input[name='saveTarget']").val("");
		$(writeForm).find("input[name='saveStep']").val("");
		$(writeForm).prepend('<input type="hidden" name="saveTarget" value="'+saveTarget+'"/>');
		$(writeForm).prepend('<input type="hidden" name="saveStep" value="'+saveStep+'"/>');
		confirmMsg = "저장 하시겠습니까?";
		if(saveStep == "STEP2") confirmMsg = "[ 기간 및 자동계산된(주,일수) ]를<br/>변경하시겠습니까?";
		$.fun.confirm({
			"content":confirmMsg,
			"action":function(){
				// 실습지원비(정규실습시간)
				if($("#newAmountMoney").length > 0) {
					$("#newAmountMoney").val($("#newAmountMoney").val().replaceAll(',',''));
				}
				// 실습지원비(연장실습시간)
				if($("#newAmountMoneyExtension").length > 0) {
					$("#newAmountMoneyExtension").val($("#newAmountMoneyExtension").val().replaceAll(',',''));
				}
				$.fun.ajax({
					"url":"/common/syworksProc.do",
					"data":$( writeForm ).serialize(),
					"success":function(data){
						data=$.fun.json_encode(data);
						console.log(data);
						if(data["result"]>0){
							$.fun.alert({
								"content":"요청하신 작업이 완료되었습니다.",
								"action":function(){
									$("#fn_js_confirm").dialog('destroy').remove();
									location.reload();
								}
							});
						}else{
							$.fun.alert({
								"content":"요청이 실패되었습니다."
							});
						}
					}
				});
				return false;
			}
		});
		return false;
	},
	// 선발대상 추가
	"addMajor":function(){
		// <!-- [copy] -->
		var jsType=$("#jsType").val();
		var maxLen = 20;
		var ul=$("#JS_ADD_MAJOR");

		if($("[name='majorLi']").length>=maxLen){
			$.fun.alert({
				"content":"더 이상 추가 할 수 없습니다."
			});
			return false;
		}
		var li=ul.children("li:first").clone();
		li.find("input,select").val("");
		li.find(".js_add_layout").html('<a href="#" onclick="return req_write.removeMajor($(this));" class="delete_entry ml_5"><span class="blind">항목삭제</span></a>');
		// <!-- //[copy] -->

		// <!-- customizing -->
		li.find(".update, .delete").remove();
		li.append('<a href="javascript:void(0)" onclick="syworks.joinCheck(this,\'insert\');" class="ml_4 btn_inner update"><span class="btn_txt">등록(141)</span></a>');
		// <!-- //customizing -->

		// <!-- [copy] -->
		ul.append(li);
		return false;
		// <!-- //[copy] -->
	},
	// 학과 onclick event 활성화
	"majorActive" : function(target) {
		target.find("li").each(function(){
			if($(this).find("input[name='jimmName[]']").attr("onclick") == undefined) {
				// <!-- [copy] -->
				$(this).find("input[name='jimmName[]']").attr("onclick","return common.exDeptManage.init({'noMajorYn':'Y','title':'선발대상검색','colgCd':$(this).prev().prev().prev() ,'colgNm':$(this).prev().prev(),'deptCode':$(this).prev(),'deptName':$(this)})");
				// <!-- //[copy] -->
			}
//			if($(this).find("input[name='jimmProfName[]']").attr("onclick") == undefined) {
				// <!-- [copy] -->
//				$(this).find("input[name='jimmProfName[]']").attr("onclick","return common.exProfessor.init({'jidProfId':$(this).prev().prev(),'jidProfName':$(this),'jidProfDept':$(this).prev()});");
				// <!-- //[copy] -->
//			}
		});
	},
	// 학과(학생지원체크여부) [필수체크 ==> 항목]
	"joinCheck" : function(req,mode) {
		// <!-- customizing / copy -->
		var jidSeq  = $("#writeForm").find("input[name='jidSeq']").val();							// 참여신청서 고유값
		var jimmSeq = $(req).parents("li").find("input[name='jimmSeq[]']").val();					// 선발대상 테이블 고유번호
		var colgCd = $(req).parents("li").find("input[name='colgCd[]']").val();						// 대학코드
		var colgNm = $(req).parents("li").find("input[name='colgNm[]']").val();						// 대학명
		var jimmCode = $(req).parents("li").find("input[name='jimmCode[]']").val();					// 학과코드
		var jimmName = $(req).parents("li").find("input[name='jimmName[]']").val();					// 학과명
		var jimmNum = $(req).parents("li").find("input[name='jimmNum[]']").val();					// 모집인원

		var sexCode = "1089";					// 성별코드(숨김)
		//var gradeCode = $(req).parents("li").find("select[name='sexCode[]'] option:selected").val();	// 성별코드(select)
		// var gradeCode = $(req).parents("li").find("input[name='gradeCode[]']").val();				// 학년코드(숨김)
		var gradeCode = $(req).parents("li").find("select[name='gradeCode[]'] option:selected").val();	// 학년코드(select)

		var jimmProfId = $(req).parents("li").find("input[name='jimmProfId[]']").val();				// 학생평가교수 아이디
		var jimmProfDept = $(req).parents("li").find("input[name='jimmProfDept[]']").val();			// 학생평가교수 학과
		var jimmProfName = $(req).parents("li").find("input[name='jimmProfName[]']").val();			// 학생평가교수 이름
		var param = {'jidSeq':jidSeq,'jimmSeq':jimmSeq,'colgCd':colgCd,'colgNm':colgNm,'jimmCode':jimmCode,'jimmName':jimmName,'jimmNum':jimmNum,'sexCode':sexCode,'gradeCode':gradeCode,'jimmProfId':jimmProfId,'jimmProfDept':jimmProfDept,'jimmProfName':jimmProfName,'mode':mode};
		// <!-- customizing / copy -->

		if(mode == "update") str = "수정";
		else if(mode == "delete") str = "삭제";

		if(mode == "insert") {
			syworks.jimmProc(param);
		}  else {
			$.fun.ajax( {
				"url" : "/common/jidJoinChk.do" ,
				"data" : {
							"jidSeq":jidSeq,
							"jimmCode":jimmCode
						} ,
				"success" : function ( data ) {
					data = $.fun.json_encode( data );
					if ( data[ "result" ] > 0 ) {
						$.fun.alert( {
							"content" : "현장실습에 참여하는 학생이 존재하여 "+ str +" 할 수 없습니다." ,
							"action" : function () {
								return false;
							}
						} );
						return false;
					} else {
						syworks.jimmProc(param);
					}
				}
			});
		}
	},
	// 학과(추가/수정/삭제)
	"jimmProc" : function(data){
		var confirmMsg = "등록 하시겠습니까?";
		if(data.mode == "update") {
			confirmMsg = "수정 하시겠습니까?";
		} else if(data.mode == "delete") {
			confirmMsg = "삭제 하시겠습니까?";
		}
		console.log(data);
		$.fun.confirm({
			"content":confirmMsg,
			"action":function(){
				$.fun.ajax({
					"url":"/common/jidDeptProc.do",
					"data":data,
					"success":function(data){
						data=$.fun.json_encode(data);
						if(data["result"]>0){
							$.fun.alert({
								"content":"요청하신 작업이 완료되었습니다.",
								"action":function(){
									location.reload();
								}
							});
						}else{
							$.fun.alert({
								"content":"요청이 실패되었습니다."
							});
						}
					}
				});
				return false;
			}
		});
		return false;
	},
	// 날짜변경 [필수체크 ==> 주간보고서,출근부]
	"jidDateUpdate" : function(changeMode,jidSeq) {
		req.workdayReday();

		$.fun.ajax( {
			"url" : "/common/jidDateChk.do" ,
			"data" : {"jidSeq":jidSeq} ,
			"success" : function ( data ) {
				data = $.fun.json_encode( data );
				if ( data[ "result" ] > 0 ) {
					$.fun.alert( {
						"content" : "현장실습에 참여하는 학생이 주간보고서 및 출근부 <br/>작성이력이 존재하여 기간변경을 할 수 없습니다.</br> 강제적으로 변경 할 경우 데이터 오류가 발생 될 수 있습니다." ,
						/*"action" : function () {
							return false;
						},*/
						"buttons" : {
							"강제변경" : function () {
								$( this ).dialog( 'destroy' ).remove();
								syworks.save('JID','STEP2');
							} ,
							"닫기" : function () {
								$( this ).dialog( 'destroy' ).remove();
							}
						}
					} );
					return false;
				} else {
					syworks.save('JID','STEP2');
				}
			}
		});

	}
}
