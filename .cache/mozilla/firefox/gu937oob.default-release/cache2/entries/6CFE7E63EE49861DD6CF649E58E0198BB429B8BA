	// 시작일과 종료일 설정된 요일별 일수 구하기
	"workdayDiff" : function(param) {
		var result = [];
	 	var sday = new Date(param.jidSDate);
	   	var eday = new Date(param.jidEDate);
   		console.log(sday + "~" + eday);
  		while(sday.getTime() <= eday.getTime()){
  			var month = (sday.getMonth()+1);
  			month = month < 10 ? '0'+month : month;
  			var day = sday.getDate();
  			day = day < 10 ? '0'+day : day;
  			result.push(sday.getFullYear() + '-' + month + '-' +  day);
  			sday.setDate(sday.getDate() + 1);
  		}
  		console.log(param);
  		var week = ['일', '월', '화', '수', '목', '금', '토'];
  		var weekCnt = 0;
	  	for(var i=0; i<result.length; i++) {
	  		if(param.monday != undefined && param.monday == new Date(result[i]).getDay()) weekCnt += 1;
	  		else if(param.tuesday != undefined && param.tuesday == new Date(result[i]).getDay()) weekCnt += 1;
	  		else if(param.wednesday != undefined && param.wednesday == new Date(result[i]).getDay()) weekCnt += 1;
	  		else if(param.thursday != undefined && param.thursday == new Date(result[i]).getDay()) weekCnt += 1;
	  		else if(param.friday != undefined && param.friday == new Date(result[i]).getDay()) weekCnt += 1;
	  		else if(param.saturday != undefined && param.saturday == new Date(result[i]).getDay()) weekCnt += 1;
	  		else if(param.sunday != undefined && param.sunday == new Date(result[i]).getDay()) weekCnt += 1;
//	  		console.log(result[i]);
//	  		console.log(week[new Date(result[i]).getDay()]);
//	  		console.log(new Date(result[i]).getDay());
	  	}
	  	console.log(weekCnt);

	   	return weekCnt;
	},
	"inspectCheck" : function() {
		var inspectCnt = 0;
		$(".inspect_detail").find("input[type='radio']:checked").each(function() { inspectCnt += 1; });
		$(".inspect_detail").find("input[type='checkbox']:checked").each(function() { inspectCnt += 1; });
		if($(".inspect_detail").find("textarea").val().length > 0) { inspectCnt += 1; }
		if(inspectCnt > 0)  $(".inspect_cont a").click();
	},
	"insuDel" : function ( jimSeq ) {
		$.fun.confirm( {
			"content" : "산재보험 증빙서류를 삭제하시겠습니까?" ,
			"action" : function () {
				$.fun.ajax( {
					"url" : "/insuProc.do" ,
					"data" : {
						"jimSeq" : jimSeq ,
						"insuDel" : "Y"
					} ,
					"success" : function ( data ) {
						data = $.fun.json_encode( data );
						console.log( data );
						if ( data[ "result" ] > -1 ) {
							$.fun.alert( {
								"content" : "삭제되었습니다." ,
								"action" : function () {
									location.reload();
								}
							} );
						} else {
							$.fun.alert( {
								"content" : "요청이 실패되었습니다."
							} );
						}
					}
				} );
				return true;
			}
		} );
		return false;
	} ,
	"insuUpload" : function ( jimSeq ) {
		common.upload( function ( data ) {
			$.fun.ajax( {
				"url" : "/insuProc.do" ,
				"data" : {
					"jimSeq" : jimSeq ,
					"insuUpload" : $.fun.json_decode( data )
				} ,
				"success" : function ( data ) {
					data = $.fun.json_encode( data );
					if ( data[ "result" ] ) {
						$.fun.alert( {
							"content" : "요청하신 작업이 완료되었습니다." ,
							"action" : function () {
								location.reload();
							}
						} );
					} else {
						$.fun.alert( {
							"content" : "요청이 실패되었습니다."
						} );
					}
				}
			} );
			return true;
		} );
		return false;
	} ,
	"insuJoinDate" : function ( jimSeq ) {
		var val = $( "#insuJoinDate"+jimSeq ).val();
		$.fun.confirm( {
			"content" : "산재보험 가입일을 저장하시겠습니까?" ,
			"action" : function () {
				$.fun.ajax( {
					"url" : "/insuProc.do" ,
					"data" : {
						"jimSeq" : jimSeq ,
						"insuJoinDate" : val
					} ,
					"success" : function ( data ) {
						data = $.fun.json_encode( data );
						console.log( data );
						if ( data[ "result" ] > -1 ) {
							$.fun.alert( {
								"content" : "저장되었습니다." ,
								"action" : function () {
									location.reload();
								}
							} );
						} else {
							$.fun.alert( {
								"content" : "요청이 실패되었습니다."
							} );
						}
					}
				} );
				return true;
			}
		} );
		return false;
	} ,
