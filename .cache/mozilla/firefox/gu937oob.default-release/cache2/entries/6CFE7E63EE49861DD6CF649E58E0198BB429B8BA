	"exProfessor":{
		"jidProfId":null,
		"jidProfName":null,
		"jidProfDept":null,
		"jidProfDeptName":null,
		"func":null,
		"jidSeq":null,
		"jimSeq" : null ,
		"jimmSeq":null,
		"mode" : null,
		"layout":function(form){
			$.fun.ajax({
				"url":"/ex/professor/index.do",
				data:form?$(form).serialize():null,
				"success":function(data){
					$.fun.layout({
						"id":"professor_layout",
						"title":"교수 검색",
						"width":610,
						"content":$(data)
					});
				}
			});
			return false;
		},
		"bind":function(param){
			common.exProfessor.jidProfId.val(param.jidProfId);
			common.exProfessor.jidProfId.change();
			if ( common.exProfessor.jimSeq != null ) {
				if ( common.exProfessor.jidProfId.parents( "tr" ).attr( "id" ).indexOf( "jim_" ) != -1 ) {
					$( "#jim_" + common.exProfessor.jimSeq ).find( "#" + common.exProfessor.jidProfName.attr( "id" ) ).val( param.jidProfName )
				}
			} else {
				common.exProfessor.jidProfName.val( param.jidProfName );
			}

			if(common.exProfessor.jidProfDept) common.exProfessor.jidProfDept.val(param.jidProfDept);
			if(common.exProfessor.jidProfDeptName) common.exProfessor.jidProfDeptName.val(param.jidProfDeptName);

			if(common.exProfessor.mode != null && common.exProfessor.mode == "type1") {
				$("#deptCode").val(param.jidProfDept)
				$("#deptName").val(param.jidProfDeptName)
			}

			$("#professor_layout").dialog('destroy').remove();
			param.jidSeq=common.exProfessor.jidSeq;
			param.jimSeq = common.exProfessor.jimSeq;
			param.jimmSeq=common.exProfessor.jimmSeq;
			if(common.exProfessor.func) common.exProfessor.func(param);
			return false;
		},
		"search":function(page){
			var form=document.commonProfessorForm;
			if(page) form.page.value=page;
			return common.exProfessor.layout(form);
		},
		"init":function(param,jidSeq){
			common.exProfessor.jidProfId=param.jidProfId;
			common.exProfessor.jidProfName=param.jidProfName;
			common.exProfessor.jidProfDept=param.jidProfDept;
			common.exProfessor.jidProfDeptName=param.jidProfDeptName;
			common.exProfessor.func=param.func;
			common.exProfessor.jidSeq=param.jidSeq;
			if(param.jimSeq != null) common.exProfessor.jimSeq = param.jimSeq;
			if(param.mode != null) common.exProfessor.mode = param.mode;
			common.exProfessor.jimmSeq=param.jimmSeq;
			common.exProfessor.layout(null);
			return false;
		},
		"keydown":function(){
			if(event.keyCode==13){
				return common.exProfessor.search(1);
			}
		}
