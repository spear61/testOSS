	"totalPrintNew" : function () {
		var checkboxInfo = $.fun.checkboxInfo( "jimSeq[]" );
		if(checkboxInfo.checked == 0) {
			$.fun.alert( {
				"content" : "학생을 선택해주세요."
			} );
			return false;
		}
		var jimSeq = checkboxInfo.checkedList;
		window.open( print_.url.totalPrintNew + "?jimSeq=" + jimSeq,"attendance" ,'toolbar=no, location=no, left=0, top=0, directories=no, status=no, menubar=no, scrollbars=yes, resizable=no, copyhistory=no, width=700px, height=850px' );
	} ,
	"FieTrainAppliFormNew" : function ( jidSeq ) {
		window.open( print_.url.FieTrainAppliFormNew + "?jidSeq=" + jidSeq ,"FieTrainAppliForm" ,'toolbar=no, location=no, left=0, top=0, directories=no, status=no, menubar=no, scrollbars=yes, resizable=no, copyhistory=no, width=670px, height=850px' );
	} ,
	"agreeInfoPrint" : function ( id, group, jsYear ) {
		if(typeof jsYear === "undefined"){
			jsYear = 0;
		}
		console.log(id);
		window.open( print_.url.agreeInfoPrint + "?id=" + id + "&group=" + group + "&jsYear=" + jsYear ,"agreeInfoPrint" ,'toolbar=no, location=no, left=0, top=0, directories=no, status=no, menubar=no, scrollbars=yes, resizable=no, copyhistory=no, width=670px, height=850px' );
	} ,
	"reviewTestPrint" : function ( jidSeq ) {
