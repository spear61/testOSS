	"commonGrpCdList":function(paramMap,target,value){
		$.fun.ajax({
			"url":"/commonGrpCdList.do",
			"data":paramMap,
			"success":function(data){
				data=$.fun.json_encode(data);
				$(target).find("option:not(option:first)").remove();
				$(data).each(function(index,val){
					$(target).append('<option value="'+val.cd+'" '+(val.cd==value?'selected="selected"':"")+'>'+val.cdNm+'</option>');
				});
				$(target).change();
			}
		});
		return false;
	},
